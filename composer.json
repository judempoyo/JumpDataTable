{
    "name": "jump/jump-datatable",
    "description": "JUmp DataTable is a lightweight and flexible library for creating dynamic data tables in PHP applications. It provides an easy way to manage, render, and manipulate tabular data with features such as sorting, filtering, and pagination.",
    "autoload": {
        "psr-4": {
            "Jump\\JumpDataTable\\": "src/"
        }
    },
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jude mpoyo",
            "email": "mpoyojude0@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "symfony/console": "^7.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jump\\JumpDataTable\\JumpDataTableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit tests/ --testdox"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1"
    },
     "bin": ["bin/jump"],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
